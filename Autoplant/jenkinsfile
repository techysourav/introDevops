    agent any
    stages {
        stage ('initialize') {
            steps {
                echo 'initializing...'
                
            }
 
          
        }
 
        stage ('Build'){
            steps{
 
               echo 'Hello World'
 
            }
        }
        
        stage ('Deploy'){
            steps{
 
               echo 'Deploying'
 
            }
        }
    }
}